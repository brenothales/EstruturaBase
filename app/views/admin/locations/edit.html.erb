<% content_for :sidebar do %>
  <%= error_message_for(@location) %>
<% end %>

<% content_for :main do  %>
  <%= onde_estou "Edição de #{@location.class.to_s}",admin_root_path,"Dashboard" %>
  <div class="box fields">
    <%= form_for(@location,:url => admin_location_path(@location),:html => {:method => :put,:multipart => true}) do |f| %>
      <%= render :partial => 'form',:locals =>{:button_label => "Editar",:f => f} %>
    <% end %>
  </div>
<% end %>

