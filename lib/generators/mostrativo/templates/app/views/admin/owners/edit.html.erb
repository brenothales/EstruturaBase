<% content_for :sidebar do %>
  <%= error_message_for(@owner) %>
<% end %>

<% content_for :main do  %>
  <%= onde_estou "Edição de #{@model_name}",admin_root_path,"Dashboard" %>
  <div class="box fields">
    <%= form_for(@owner,:url => admin_menu_path(@owner),:html => {:method => :put,:multipart => true}) do |f| %>
      <%= render :partial => 'form',:locals =>{:button_label => "Editar",:f => f} %>
    <% end %>
  </div>
<% end %>

